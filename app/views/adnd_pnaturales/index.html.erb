<div class="contenedor">
  <h1>Listado de lugares de Parques naturales</h1>
  <table>
    <thead>
      <tr>
        <th>Título</th>
        <th>Subtítulo</th>
        <th>Ruta</th>
        <th colspan="3">Acciones</th>
      </tr>
    </thead>

    <tbody>
      <% @adnd_pnaturales.each do |adnd_pnaturale| %>
        <tr>
          <td><%= adnd_pnaturale.titulo %></td>
          <td><%= adnd_pnaturale.subtitulo %></td>
          <td><%= adnd_pnaturale.ruta %></td>
          <td><%= link_to 'Mostrar', adnd_pnaturale %></td>
          <td><%= link_to 'Editar', edit_adnd_pnaturale_path(adnd_pnaturale) %></td>
          <td><%= link_to 'Eliminar', adnd_pnaturale, method: :delete, data: { confirm: '¿Está seguro de eliminar esta entrada?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
  <div class="enlace">
    <%= link_to 'Nueva entrada', new_adnd_pnaturale_path %>
    <%= link_to 'Atrás', root_path %>
  </div>
  <p id="notice"><%= notice %></p>
</div>