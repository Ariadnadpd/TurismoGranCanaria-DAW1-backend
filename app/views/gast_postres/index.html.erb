<p id="notice"><%= notice %></p>

<h1>Gast Postres</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Subtitulo</th>
      <th>Localizacion</th>
      <th>Telefono</th>
      <th>Rss facebook</th>
      <th>Rss instagram</th>
      <th>Informacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gast_postres.each do |gast_postre| %>
      <tr>
        <td><%= gast_postre.titulo %></td>
        <td><%= gast_postre.subtitulo %></td>
        <td><%= gast_postre.localizacion %></td>
        <td><%= gast_postre.telefono %></td>
        <td><%= gast_postre.rss_facebook %></td>
        <td><%= gast_postre.rss_instagram %></td>
        <td><%= gast_postre.informacion %></td>
        <td><%= link_to 'Show', gast_postre %></td>
        <td><%= link_to 'Edit', edit_gast_postre_path(gast_postre) %></td>
        <td><%= link_to 'Destroy', gast_postre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gast Postre', new_gast_postre_path %>
