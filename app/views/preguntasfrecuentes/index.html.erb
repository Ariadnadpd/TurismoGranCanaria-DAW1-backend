<p id="notice"><%= notice %></p>

<h1>Preguntasfrecuentes</h1>

<table>
  <thead>
    <tr>
      <th>Pregunta</th>
      <th>Respuesta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @preguntasfrecuentes.each do |preguntasfrecuente| %>
      <tr>
        <td><%= preguntasfrecuente.pregunta %></td>
        <td><%= preguntasfrecuente.respuesta %></td>
        <td><%= link_to 'Show', preguntasfrecuente %></td>
        <td><%= link_to 'Edit', edit_preguntasfrecuente_path(preguntasfrecuente) %></td>
        <td><%= link_to 'Destroy', preguntasfrecuente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preguntasfrecuente', new_preguntasfrecuente_path %>
