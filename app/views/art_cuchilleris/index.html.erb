
<h1>Listado de sitios de Cuchillos canarios</h1>

<table>
  <thead>
    <tr>
      <th>Título</th>
      <th>Subtítulo</th>
      <th>Localización</th>
      <th>Teléfono</th>
      <th>Información</th>
      <th>Página de Facebook</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @art_cuchilleris.each do |art_cuchilleri| %>
      <tr>
        <td><%= art_cuchilleri.titulo %></td>
        <td><%= art_cuchilleri.subtitulo %></td>
        <td><%= art_cuchilleri.localizacion %></td>
        <td><%= art_cuchilleri.telefono %></td>
        <td><%= art_cuchilleri.informacion %></td>
        <td><%= art_cuchilleri.rss_facebook %></td>
        <td><%= link_to 'Mostrar', art_cuchilleri %></td>
        <td><%= link_to 'Editar', edit_art_cuchilleri_path(art_cuchilleri) %></td>
        <td><%= link_to 'Eliminar', art_cuchilleri, method: :delete, data: { confirm: '¿Está seguro de eliminar esta entrada?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva entrada', new_art_cuchilleri_path %>
<p id="notice"><%= notice %></p>

