<div class="contenedor">
  <h1>Playa: <%= @adnd_playa.subtitulo %></h1>

  <p>
    <strong>Título:</strong>
    <%= @adnd_playa.titulo %>
  </p>

  <p>
    <strong>Subtítulo:</strong>
    <%= @adnd_playa.subtitulo %>
  </p>

  <p>
    <strong>Localización:</strong>
    <%= @adnd_playa.localizacion %>
  </p>
  <div class="enlace">
    <%= link_to 'Editar', edit_adnd_playa_path(@adnd_playa) %>
    <%= link_to 'Atrás', adnd_playas_path %>
  </div>
  <p id="notice"><%= notice %></p>
</div>